{
  "name": "revieweer",
  "version": "2.1.1",
  "description": "www.revieweer.com Official Website and Server",
  "main": "server/build/server.js",
  "engines": {
    "node": "8.5.0"
  },
  "scripts": {
    "start": "node start --prefix server",
    "test": "eslint .",
    "precommit": "npm run test && build-client && npm version patch",
    "prepush": "npm run test",
    "postinstall": "npm i --prefix server && npm run build",
    "build": "npm run build:server",
    "build:client": "cd ./client && npm run build && cd ..",
    "build:server": "npm run clean:server && babel -d ./server/build ./server/src -s",
    "clean:server": "rm -rf ./server/build && mkdir ./server/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amazingandyyy/revieweer.git"
  },
  "keywords": [
    "marketing",
    "seo",
    "ecommerce",
    "online shopping",
    "review",
    "referral"
  ],
  "author": "Amazingandyyy <amazingandyyy@gmail.com> (Andy Chen)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amazingandyyy/revieweer/issues"
  },
  "homepage": "https://github.com/amazingandyyy/revieweer#readme",
  "devDependencies": {
    "eslint": "^4.14.0",
    "eslint-config-logux": "^17.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-es5": "^1.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.10.0"
  }
}
